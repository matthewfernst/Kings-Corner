		"dotenv": "^10.0.0",
		"electron-squirrel-startup": "^1.0.0",
		"express": "^4.17.1",
		"framer-motion": "^5.2.1",
		"google-auth-library": "^7.10.2",
		"graphql": "^15.0.0",
