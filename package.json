{
	"name": "kings-corner",
	"author": "Meta Games",
	"version": "2.0.0",
	"description": "A Modern Chess Web App",
	"repository": "https://github.com/M3tanym/Kings-Corner",
	"license": "MIT",
	"main": "client/main.cjs",
	"scripts": {
		"test:unit": "mocha --require @babel/register --recursive",
		"test:coverage": "c8 --all --check-coverage --lines 70 --include 'server/' npm run test:unit",
		"test": "npm run test:coverage",
		"server": "webpack --watch --config server/webpack.config.mjs",
		"mockServer": "MOCK=true npm run server",
		"startElectron": "webpack serve --config client/webpack.electron.config.mjs",
		"startWebsite": "webpack serve --config client/webpack.website.config.mjs",
		"start": "concurrently --kill-others-on-fail \"npm:server\" \"npm:startWebsite\"",
		"buildServer": "webpack --config server/webpack.config.mjs",
		"buildWebsite": "webpack --config client/webpack.website.config.mjs",
		"build": "concurrently --kill-others-on-fail \"npm:buildServer\" \"npm:buildWebsite\"",
		"publishElectron": "electron-forge publish"
	},
	"config": {
		"forge": "client/forge.cjs"
	},
	"dependencies": {
		"@apollo/client": "^3.4.16",
		"@babel/plugin-transform-runtime": "^7.16.0",
